<Window x:Class="VirtualDesktopsManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VirtualDesktopsManager"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="400" Closing="Window_Closing">
    <Grid>
        <Button x:Name="EnableButton" Content="Button" HorizontalAlignment="Left" Margin="100,0,0,0" VerticalAlignment="Center" Height="100" Width="200" FontSize="48" Click="EnableButton_Click"/>
    </Grid>
</Window>
